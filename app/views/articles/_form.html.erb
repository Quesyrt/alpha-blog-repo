<%= form_for @article do |f| %>

    <P>
    <%=  f.label :title%> <br />
    <%=  f.text_field :title%>
    </P>
    
    <P>
    <%=  f.label :description%> <br />
    <%=  f.text_area :description%>
    </P>
    
    <p>
        <%= f.submit %>
    </p>

<% end %>

<%= link_to "Retour aux articles", articles_path %>